#! /usr/bin.env bash

pipeline{
    agent any
    stages{
        stage("test"){
            steps{
                script{
                    echo "Testing..."
                }
            }
            
        }

        stage("build"){
            steps{
                script{
                    echo "Building..."
                }
            }
            
        }

        stage("deploy"){
            steps{
                script{
                    echo "Deploying..."
                }
            }
            
        }
    }
}